<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="beer.dacelo.dev.aoq2023.PrimaryController">
   <children>
      <Label text="Select your Challenge" />
      <Button fx:id="aoc2022button" text="Advent of Code 2022" onAction="#switchToAoC2022"/>
      <Button fx:id="aoq2023button" text="Advent of Qode 2023" onAction="#switchToAoQ2023"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
